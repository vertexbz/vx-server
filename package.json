    "tslib": "1.9.3",
    "tslint": "5.12.0",
    "tslint-config-standard": "8.0.1",
    "tslint-etc": "1.2.12",
    "typedoc": "^0.14.2",
    "typedoc-plugin-markdown": "^2.0.8",
