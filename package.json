{
  "name": "vx-experess-server",
  "version": "0.1.2",
  "license": "MIT",
  "description": "VX wrapper for express server + plugins",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typings": "./dist/index.d.ts",
  "sideEffects": false,
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.1",
